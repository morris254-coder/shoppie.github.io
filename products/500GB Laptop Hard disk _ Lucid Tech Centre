.form-control, .input-text, input[type="email"], input[type="password"], input[type="search"], input[type="text"]:not(.form-control):not(input[type="email"]):not(input[type="url"]):not(input[type="password"]):not(input[type="search"]):not(textarea):not(.input-text), input[type="url"], textarea { display: block; width: 100%; padding: 0.594rem 1rem; font-size: 0.875rem; font-weight: 400; line-height: 1.714; color: rgb(51, 62, 72); background-color: rgb(255, 255, 255); background-clip: padding-box; border: 1px solid rgb(221, 221, 221); appearance: none; border-radius: 1.375rem; transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out; }

